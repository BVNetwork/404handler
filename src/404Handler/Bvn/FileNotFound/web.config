<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<location path="admin">
		<system.web>
			<globalization requestEncoding="utf-8" responseEncoding="utf-8" />
			<authorization>
				<allow roles="WebAdmins, Administrators" />
				<deny users="*" />
			</authorization>
		</system.web>
	</location>
</configuration>
